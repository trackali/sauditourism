<?xml version='1.0' encoding='utf-8'?>
<widget id="saudi.tourism" version="0.0.1" xmlns="http://www.w3.org/ns/widgets"
	xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>saudi Tourism</name>
	<description>This Application alow you to get all necessary information about the saudi Arabia tourism information and it will work as a gidance for you about the different activeties, places to visit and much more. </description>
	<author href="http://www.tripoadviser.com" email="apps@tripoadviser.com">Tripoadviser</author>

	<content src="index.html" />
	<!-- Whitelist configuration. Refer to https://cordova.apache.org/docs/en/edge/guide_appdev_whitelist_index.md.html -->
	<plugin name="cordova-plugin-whitelist" spec="1" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<platform name="android">
		<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
	</platform>
	
<engine name="android" spec="6.1.0" /></widget>
